      "rdfs:subClassOf" : "schema:CreativeWork",
      "schema:isPartOf" : "http:bib.schema.org"
   },
   "properties" : {
      "schema:pageEnd" : {
         "owl:equivalentProperty" : "bibo:pageEnd",
